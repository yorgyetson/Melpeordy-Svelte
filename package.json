		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.5",
		"tslib": "^2.4.1",
		"typescript": "^4.9.5",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@apollo/client": "^3.7.8",
		"graphql": "^16.6.0",
		"graphql-ws": "^5.11.3",
		"jwt-decode": "^3.1.2"
	}
